generator client {
  provider      = "prisma-client-js"
  output        = "../../../../node_modules/@gen/client/users"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_USERS")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String
}
