generator client {
  provider      = "prisma-client-js"
  output        = "../../../../node_modules/@gen/client/restaurants"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_RESTAURANTS")
}

model restaurant {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  name         String?
  owner        String
  priceRange   String?
  currentOffer Int?
  image        String?
  phone        String?
  address      String?
  siret        String?
  category     String?
}
