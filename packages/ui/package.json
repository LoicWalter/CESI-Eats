    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "jwt-decode": "^4.0.0",
    "next": "^14.1.1",
